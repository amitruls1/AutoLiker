{
    "name": "Auto Date Liker",
    "version": "1.03",
    "description": "This extension is use to start auto like on Tinder, Bumble and OKCupid",
    "manifest_version": 2,
    "minimum_chrome_version": "46",
    "permissions": [
        "activeTab"
      ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Date Liker",
        "default_icon": "icon.png"
    }
}